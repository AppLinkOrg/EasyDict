    </view>

    <button style='margin-top:60rpx' hover-class='evaluation_hover' class='evaluation txt-white '>点击翻译</button>
  </view>

  <view class='text-center flex-row flex-column margin-top-80' hidden='true'>
    <image class='margin-left-40 img' src='{{uploadpath}}resource/{{res.notyet}}'></image>
