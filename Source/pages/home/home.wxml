<view>

  <swiper indicator-dots='true' autoplay='false' interval='3000' duration='500' circular='true' style='width:750rpx;height:250rpx;'>
    <swiper-item wx:for="{{indexbanner}}" wx:key="k">
      <view>
        <!-- mode="aspectFill" -->
        <image src='{{uploadpath}}indexbanner/{{item.img}}' bindtap='viewPhoto' id='{{uploadpath}}indexbanner/{{item.img}}' style='width:750rpx;height:350rpx;'></image>
      </view>
    </swiper-item>
  </swiper>
</view>

<view class='margin-top-20 '>
  <view class='padding-20 txt-black txt-bold'>英汉翻译</view>

  <view class=''>
