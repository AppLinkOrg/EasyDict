  </view>

  <view style='' class=' mid margin-top-80'>
    <view class='padding-20 txt-black'>简易音标</view>
    <view class='flex-row flex-column '>
      <view class='text-center h3 txt-black txt-bold' style='margin-bottom:70rpx;margin-top:30rpx;'>PICKER
